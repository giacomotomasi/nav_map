  <node name="camera_tf_broadcaster" pkg="realsense_devel" type="camera_tf_broadcaster" output="screen" />
  <node name="rviz" pkg="rviz" type="rviz" output="screen" args="-d $(find nav_map)/rviz/map_bag.rviz" />
  
</launch>